import "!stdlib.src";

let main = () => i64 {
	let fib = (n: i64) => i64 {
		if n == 0 {
			return 0;
		};
		if n == 1 {
			return 1;
		};
		return fib(n-2) + fib(n-1);
	};

	let result = fib(30);
	printf("%d\n", result);
	return result;
};

/*

Expected stdout:

832040

Expected return: 832040

