/* As you see, the language supports variable scopes */
extern printf = (*i64, varargs) => i64;

let x = 18;

let foo = () => i64 {
	let x = 1;
	printf("%d\n", x);
	return x;
};

let main = () => i64 {
	printf("%d\n", x);
	let x = 2;
	let y = foo();
	printf("%d\n", x);
	return x + y;
};

/*

Expected stdout:

18
1
2

Expected return: 3

*/
